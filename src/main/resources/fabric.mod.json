{
  "schemaVersion": 1,
  "id": "nylemod",
  "version": "${version}",
  "name": "nylemod",
  "description": "Example mod using Fabric 1.19",
  "authors": [
    "nylestroke"
  ],
  "contact": {
    "homepage": "https://github.com/nylestroke/nylemod-example"
  },
  "license": "MIT",
  "icon": "assets/nylemod/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      {
        "adapter": "kotlin",
        "value": "me.nylestroke.nylemod.client.NylemodClient"
      }
    ],
    "main": [
      {
        "adapter": "kotlin",
        "value": "me.nylestroke.nylemod.NylemodExample"
      }
    ]
  },
  "mixins": [
    "nylemod.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.12",
    "fabric": "*",
    "minecraft": "~1.19",
    "java": ">=17",
    "fabric-language-kotlin": ">=1.9.0+kotlin.1.8.0"
  },
  "suggests": {
    "another-mod": "*"
  }
}
